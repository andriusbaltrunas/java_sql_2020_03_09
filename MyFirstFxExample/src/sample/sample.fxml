   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="399.0" prefWidth="467.0">
         <children>
            <Label fx:id="emptyLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="8.0" layoutY="14.0" prefHeight="30.0" prefWidth="455.0" textFill="#ee1111">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="nameTextField" layoutX="26.0" layoutY="134.0" promptText="Your name" />
            <TextField fx:id="lastnameTextField" layoutX="26.0" layoutY="175.0" promptText="Your lastname" />
            <Button layoutX="208.0" layoutY="220.0" mnemonicParsing="false" onAction="#sayHello" text="Button" />
         </children>
      </Pane>
   </children>
</GridPane>
